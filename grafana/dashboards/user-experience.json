    "list": [
      {
        "datasource": {
          "type": "yesoreyeram-infinity-datasource",
          "uid": "efba5wxx8ub5sa"
        },
        "enable": true,
        "iconColor": "#FF9830",
        "name": "Releases",
        "target": {
          "refId": "releases",
          "type": "json",
          "format": "dataframe",
          "source": "url",
          "parser": "backend",
          "url": "/release",
          "url_options": {
            "method": "GET",
            "data": "",
            "params": [
              {
                "key": "filter[objects]",
                "value": "[{\"name\":\"created_at\",\"op\":\"ge\",\"val\":\"${__timeFrom:date:iso}\"},{\"name\":\"created_at\",\"op\":\"le\",\"val\":\"${__timeTo:date:iso}\"},{\"name\":\"deployment_id\",\"op\":\"in\",\"val\":${deployed_ids:json}}]"
              }
            ]
          },
          "root_selector": "data",
          "columns": [
            {
              "selector": "attributes.created_at",
              "text": "time",
              "type": "timestamp"
            },
            {
              "selector": "id",
              "text": "text",
              "type": "string"
            },
            {
              "selector": "attributes.build_num",
              "text": "title",
              "type": "number"
            }
          ]
        }
      },
      {
        "datasource": {
          "type": "victoriametrics-logs-datasource",
          "uid": "ffb8kdooypz40e"
        },
        "enable": true,
        "iconColor": "#5794F2",
        "name": "Deploy Start",
        "target": {
          "refId": "deploy_start",
          "expr": "action:deployment_start environment:$environment stack:in($stack) service:in($service)"
        }
      },
      {
        "datasource": {
          "type": "victoriametrics-logs-datasource",
          "uid": "ffb8kdooypz40e"
