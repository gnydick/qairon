        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "ffb8jnum7rx8ge"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "min": 0,
          "max": 100,
          "links": [
            {
              "title": "Service Detail",
              "url": "/d/service-detail/service-detail?var-environment=${environment}&var-stack=${__field.labels.stack}&var-service=${__field.labels.service}&${__url_time_range}",
              "targetBlank": true
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc",
          "maxHeight": 300
