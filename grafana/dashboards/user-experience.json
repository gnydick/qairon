          "maxHeight": 300,
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "expr": "sum by (service, stack) (count_over_time(cache_hit{environment=\"$environment\", region=\"$region\", stack=\"$stack\", service=~\"$service\", action=~\"$action\", is_write=\"false\"}[$__rate_interval] == 1)) / sum by (service, stack) (count_over_time(cache_hit{environment=\"$environment\", region=\"$region\", stack=\"$stack\", service=~\"$service\", action=~\"$action\", is_write=\"false\"}[$__rate_interval])) * 100",
          "legendFormat": "{{service}}",
          "refId": "A"
