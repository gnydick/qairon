  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_VICTORIAMETRICS}"
        },
        "query": "label_values(request_duration_ms, environment)",
        "current": {
          "text": "prod",
          "value": "prod"
        },
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "sort": 1
      },
      {
        "name": "region",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_VICTORIAMETRICS}"
        },
        "query": "label_values(request_duration_ms{environment=\"$environment\"}, region)",
        "current": {
          "text": "All",
          "value": "$__all",
          "selected": true
        },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "sort": 1
      },
      {
        "name": "stack",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_VICTORIAMETRICS}"
        },
        "query": "label_values(request_duration_ms{environment=\"$environment\"}, stack)",
        "current": {
          "text": "All",
          "value": "$__all",
          "selected": true
        },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "sort": 1
      },
      {
        "name": "service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_VICTORIAMETRICS}"
        },
        "query": "label_values(request_duration_ms{environment=\"$environment\", stack=~\"$stack\"}, service)",
        "current": {
          "text": "All",
          "value": "$__all",
          "selected": true
        },
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "sort": 1
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
