        {
          "refId": "A",
          "expr": "(level:ERROR OR level:WARN) environment:$environment stack:in($stack) service:in($service) | stats by(error_message) count() as count | sort by(count) desc | limit 20"
        }
      ]
    },
    {
      "collapsed": false,
