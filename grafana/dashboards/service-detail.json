{
  "__inputs": [
    {
      "name": "DS_VICTORIAMETRICS",
      "label": "VictoriaMetrics",
      "description": "VictoriaMetrics Prometheus-compatible datasource",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    },
    {
      "name": "DS_VICTORIALOGS",
      "label": "VictoriaLogs",
      "description": "VictoriaLogs datasource",
      "type": "datasource",
      "pluginId": "victoriametrics-logs-datasource",
      "pluginName": "VictoriaLogs"
    },
    {
      "name": "DS_TEMPO",
      "label": "Tempo",
      "description": "Grafana Tempo datasource",
      "type": "datasource",
      "pluginId": "tempo",
      "pluginName": "Tempo"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "12.3.1"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "datasource",
      "id": "victoriametrics-logs-datasource",
      "name": "VictoriaLogs",
      "version": "1.0.0"
    },
    {
      "type": "datasource",
      "id": "tempo",
      "name": "Tempo",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "piechart",
      "name": "Pie chart",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    },
    {
      "type": "panel",
      "id": "logs",
      "name": "Logs",
      "version": ""
    }
  ],
  "id": null,
  "uid": "service-detail",
  "title": "Service Detail",
  "description": "Single-service drill-down with actions, errors, latency, and dependencies",
  "tags": ["social-platform", "sre"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [
    {
      "asDropdown": true,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": ["social-platform"],
      "targetBlank": false,
      "title": "SRE Dashboards",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Service Stats",
      "type": "row"
    },
    {
      "id": 1,
      "title": "Request Rate",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) / $__rate_interval",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 2,
      "title": "Error Rate %",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 4, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\", success=\"false\"}[$__rate_interval])) / sum(count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) * 100",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 3,
      "title": "P50 Latency",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 8, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 500 },
              { "color": "red", "value": 1000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.5, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 4,
      "title": "P95 Latency",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 800 },
              { "color": "red", "value": 2000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.95, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 5,
      "title": "Cache Hit Rate",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 16, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "green", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time(cache_hit{environment=~\"$environment\", region=~\"$region\", service=~\"$service\", is_write=\"false\"}[$__rate_interval] == 1)) / sum(count_over_time(cache_hit{environment=~\"$environment\", region=~\"$region\", service=~\"$service\", is_write=\"false\"}[$__rate_interval])) * 100",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 6,
      "title": "Avg DB Queries/Req",
      "type": "stat",
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 20 }
            ]
          },
          "decimals": 1
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(db_query_count{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": ""
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 },
      "id": 101,
      "title": "Rate & Errors",
      "type": "row"
    },
    {
      "id": 10,
      "title": "Request Rate by Action",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal", "group": "A" },
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean"] }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) / $__rate_interval",
          "legendFormat": "{{action}}"
        }
      ]
    },
    {
      "id": 11,
      "title": "Error Rate by Action",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\", success=\"false\"}[$__rate_interval])) / sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) * 100",
          "legendFormat": "{{action}}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "id": 102,
      "title": "Latency",
      "type": "row"
    },
    {
      "id": 20,
      "title": "P50 / P95 / P99 Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "P99" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "P95" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }]
          },
          {
            "matcher": { "id": "byName", "options": "P50" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.5, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": "P50"
        },
        {
          "refId": "B",
          "expr": "quantile_over_time(0.95, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": "P95"
        },
        {
          "refId": "C",
          "expr": "quantile_over_time(0.99, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])",
          "legendFormat": "P99"
        }
      ]
    },
    {
      "id": 21,
      "title": "P95 by Action",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.95, request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval]) by (action)",
          "legendFormat": "{{action}}"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "id": 103,
      "title": "Error Breakdown",
      "type": "row"
    },
    {
      "id": 30,
      "title": "Errors by Source",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 23 },
      "datasource": { "type": "victoriametrics-logs-datasource", "uid": "${DS_VICTORIALOGS}" },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "pieType": "donut",
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["value"] }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "level:ERROR service:$service environment:$environment | extract \"<source>:\" from error_source | stats by(source) count() as errors"
        }
      ]
    },
    {
      "id": 31,
      "title": "Error Types Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 23 },
      "datasource": { "type": "victoriametrics-logs-datasource", "uid": "${DS_VICTORIALOGS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal", "group": "A" },
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "level:ERROR service:$service environment:$environment | stats by(_time:5m, error_type) count() as errors"
        }
      ]
    },
    {
      "id": 32,
      "title": "Top Error Messages",
      "type": "table",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 23 },
      "datasource": { "type": "victoriametrics-logs-datasource", "uid": "${DS_VICTORIALOGS}" },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "count", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "level:ERROR service:$service environment:$environment | stats by(error_message) count() as count | sort by (count) desc | limit 20"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "id": 104,
      "title": "Actions",
      "type": "row"
    },
    {
      "id": 40,
      "title": "All Actions by Volume & Error Rate",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Value #B" },
            "properties": [
              { "id": "displayName", "value": "Error Rate %" },
              { "id": "unit", "value": "percent" },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    { "color": "green", "value": null },
                    { "color": "yellow", "value": 1 },
                    { "color": "red", "value": 5 }
                  ]
                }
              },
              { "id": "custom.displayMode", "value": "color-background" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value #A" },
            "properties": [
              { "id": "displayName", "value": "Request Rate" },
              { "id": "unit", "value": "reqps" }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Request Rate", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) / $__rate_interval",
          "legendFormat": "{{action}}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\", success=\"false\"}[$__rate_interval])) / sum by (action) (count_over_time(request_duration_ms{environment=~\"$environment\", region=~\"$region\", service=~\"$service\"}[$__rate_interval])) * 100",
          "legendFormat": "{{action}}",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "id": 105,
      "title": "Dependencies & Deployments",
      "type": "row"
    },
    {
      "id": 50,
      "title": "Upstream Dependency Errors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 41 },
      "datasource": { "type": "victoriametrics-logs-datasource", "uid": "${DS_VICTORIALOGS}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": { "mode": "normal", "group": "A" },
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:dependency* service:$service environment:$environment | stats by(_time:5m, error_source) count() as errors"
        }
      ]
    },
    {
      "id": 51,
      "title": "Recent Deployments",
      "type": "logs",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 41 },
      "datasource": { "type": "victoriametrics-logs-datasource", "uid": "${DS_VICTORIALOGS}" },
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(action:deployment_start OR action:deployment_complete) service:$service environment:$environment | sort by (_time desc) | limit 50"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
        "query": "label_values(request_duration_ms, environment)",
        "current": { "text": "prod", "value": "prod" },
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "sort": 1
      },
      {
        "name": "region",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
        "query": "label_values(request_duration_ms{environment=\"$environment\"}, region)",
        "current": {},
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "sort": 1,
        "allValue": ".*"
      },
      {
        "name": "service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_VICTORIAMETRICS}" },
        "query": "label_values(request_duration_ms{environment=\"$environment\"}, service)",
        "current": {},
        "refresh": 2,
        "includeAll": false,
        "multi": false,
        "sort": 1
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "refresh": "",
  "schemaVersion": 39,
  "version": 0
}