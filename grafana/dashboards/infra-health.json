  "id": null,
  "uid": "infra-health",
  "title": "Infrastructure Health",
  "description": "Shared infrastructure monitoring and cascade visualization",
  "tags": [
    "social-platform",
    "sre"
  ],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [
    {
      "asDropdown": true,
      "icon": "external link",
      "includeVars": true,
      "keepTime": true,
      "tags": [
        "social-platform"
      ],
      "targetBlank": false,
      "title": "SRE Dashboards",
      "type": "dashboards"
    }
  ],
  "panels": [
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 100,
      "title": "Infrastructure Stats",
      "type": "row"
    },
    {
      "id": 1,
      "title": "Total Infra Errors",
      "type": "stat",
      "gridPos": {
        "h": 3,
        "w": 8,
        "x": 0,
        "y": 1
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment | stats count() as total"
        }
      ]
    },
    {
      "id": 2,
      "title": "Total Cascade Errors",
      "type": "stat",
      "gridPos": {
        "h": 3,
        "w": 8,
        "x": 8,
        "y": 1
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:dependency* environment:$environment | stats count() as total"
        }
      ]
    },
    {
      "id": 3,
      "title": "Affected Services",
      "type": "stat",
      "gridPos": {
        "h": 3,
        "w": 8,
        "x": 16,
        "y": 1
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 15
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment | stats by(service) count() | stats count() as affected_services"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "id": 101,
      "title": "Timeline",
      "type": "row"
    },
    {
      "id": 10,
      "title": "Infrastructure Errors by Component Over Time",
      "type": "timeseries",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30,
            "stacking": {
              "mode": "normal",
              "group": "A"
            },
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "sum",
            "max"
          ]
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment error_source:~\"$component\" | stats by(_time:5m, error_source) count() as errors"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 15
      },
      "id": 102,
      "title": "Per-Component",
      "type": "row"
    },
    {
      "id": 20,
      "title": "Error Count by Component",
      "type": "bargauge",
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 500
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment | stats by(error_source) count() as errors | sort by(errors) desc"
        }
      ]
    },
    {
      "id": 21,
      "title": "Affected Services per Component",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "errors",
            "desc": true
          }
        ]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment error_source:~\"$component\" | stats by(error_source, service) count() as errors | sort by(errors) desc | limit 50"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 26
