            "sum"
          ]
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:dependency* environment:$environment | stats by(_time:5m, service) count() as errors"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 37
      },
      "id": 104,
      "title": "Deep Dive",
      "type": "row"
    },
    {
      "id": 40,
      "title": "Error Logs for Selected Component",
      "type": "logs",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 38
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:infrastructure* environment:$environment error_source:~\"$component\" | sort by (_time desc) | limit 200"
        }
      ]
