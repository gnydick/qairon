        },
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "sum"
          ]
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "error_source:dependency* environment:$environment | stats by(_time:5m, error_source) count() as errors"
        }
      ]
    },
    {
      "id": 31,
      "title": "Cascade by Affected Service",
      "type": "timeseries",
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 27
      },
      "datasource": {
        "type": "victoriametrics-logs-datasource",
        "uid": "${DS_VICTORIALOGS}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20,
            "stacking": {
              "mode": "normal",
